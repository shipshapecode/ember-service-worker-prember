{
  "name": "ember-service-worker-prember",
  "version": "0.4.0",
  "description": "An Ember Service Worker plugin that caches the index.html files for each prember route",
  "keywords": [
    "ember-addon",
    "ember-service-worker-plugin",
    "prember",
    "prerender"
  ],
  "license": "MIT",
  "author": "Robert Wagner",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/shipshapecode/ember-service-worker-prember",
  "dependencies": {
    "broccoli-merge-trees": "^3.0.2",
    "broccoli-plugin": "^1.3.1",
    "rev-hash": "^2.0.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-service-worker"
    ],
    "after": [
      "prember"
    ]
  }
}
